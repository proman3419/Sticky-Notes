<Application x:Class="Sticky_Notes.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:Sticky_Notes"
             StartupUri="Resources\Code\Windows\MainWindow.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="Resources\Code\XAML Resources\Brushes.xaml" />
                <ResourceDictionary Source="Resources\Code\XAML Resources\Images.xaml" />
                <ResourceDictionary Source="Resources\Code\XAML Resources\Styles.xaml" />
                <ResourceDictionary Source="Resources\Code\XAML Resources\MainWindowStyles.xaml" />
                <ResourceDictionary Source="Resources\Code\XAML Resources\SettingsWindowStyles.xaml" />
                <ResourceDictionary Source="Resources\Code\Themes\Yellow.xaml" />
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
